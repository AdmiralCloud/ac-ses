  "dependencies": {
    "async": "^2.6.1",
    "aws-sdk": "^2.250.1",
    "lodash": "^4.x"
  },
  "devDependencies": {
    "ac-conventional-changelog-template": "github:mmpro/ac-conventional-changelog-template",
    "corp-semantic-release": "github:mmpro/corp-semantic-release",
    "cz-customizable": "github:mmpro/cz-customizable",
    "eslint": "^4.x",
    "eslint-config-standard": "^11.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-node": "^6.x",
    "eslint-plugin-promise": "^3.x",
    "eslint-plugin-standard": "^3.x",
    "expect": "^22.x",
    "ioredis": "^3.2.2",
    "mocha": "^5.x",
    "mocha-jenkins-reporter": "github:mmpro/mocha-jenkins-reporter",
    "mocha-junit-reporter": "^1.x",
    "mocha-multi-reporters": "^1.x"
